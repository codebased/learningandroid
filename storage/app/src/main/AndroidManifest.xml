<manifest package="com.imcodebased.storage"
          xmlns:android="http://schemas.android.com/apk/res/android">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".LPreferenceActivity"/>
        <activity android:name=".InternalStorageActivity"/>
        <activity android:name=".UserPreferenceActivity"/>
        <activity android:name=".ManualPreferenceActivity"/>


    </application>


</manifest>
